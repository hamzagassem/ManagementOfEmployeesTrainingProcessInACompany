QPushButton
{
padding: 0px 0px;
  font-size: 15px;
  font-weight: normal;
  line-height: 1.4;
  border: none;
  border-radius:4 px;
color: #fff;
  background-color: #e74c3c;
  -webkit-transition: border .25s linear, color .25s linear, background-color .25s linear;
          transition: border .25s linear, color .25s linear, background-color .25s linear;
}
QPushButton:pressed
{
color: #fff;
  background-color: #ec7063;
  border-color: #ec7063;
}
QPushButton:hover
{
color: #fff;
  background-color: #ec7063;
  border-color: #ec7063;
}



